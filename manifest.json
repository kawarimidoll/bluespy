{
  "manifest_version": 3,
  "name": "Bluespy",
  "author": "kawarimidoll",
  "version": "1.2.2",
  "description": "Reveal links in Bluesky.",
  "content_scripts": [
    {
      "matches": [
        "https://bsky.app/",
        "https://bsky.app/profile/*",
        "https://staging.bsky.app/",
        "https://staging.bsky.app/profile/*"
      ],
      "js": ["./scripts/content_scripts.js"]
    }
  ],
  "icons": {
    "16": "./images/bluespy-16.png",
    "128": "./images/bluespy-128.png"
  }
}
